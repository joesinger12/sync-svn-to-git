  INPUTCLASS 
    NotInput
  SIZING_PROPOSED : T24N_2016 T24N_2019
    SumRevRef( Spc:ThrmlZnRef, Spc:RetailArea )
  SIZING_PROPOSED : T24N
    if( Bldg:AboveGrdStoryCntForBaseSysMap < 3 .OR.
        ( Bldg:AboveGrdStoryCntForBaseSysMap = 3 .AND.
          Bldg:NonResAreaForBaseSysMap < 25000 ) )
    then 
      SumRevRef( Spc:ThrmlZnRef, Spc:RetailArea )
    else 0
    endif
  SIZING_BASELINE
    zp:RetailArea
ENDRULE
