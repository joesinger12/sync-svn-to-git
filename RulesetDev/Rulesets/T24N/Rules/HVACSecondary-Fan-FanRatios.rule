        LocalCompAssigned( PriAirCondgSysRef[1]) .AND.
        IfValidAnd( PriAirCondgSysRef[1]:PropExhFanPwr > 0 ) )
    then PropExhFanPwr / PriAirCondgSysRef[1]:PropExhFanPwr
    else 0
    endif
