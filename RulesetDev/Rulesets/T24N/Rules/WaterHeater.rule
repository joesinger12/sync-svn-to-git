      if( Type = "HeatPumpSplit" .AND. IfValidAnd( InpPwr > 0 ) )
      then
        InpPwr * 0.85 * 1000
      else if( Type = "HeatPumpPackaged" .AND. IfValidAnd( InpPwr > 0 ) )
      then
        InpPwr * 0.91 * 1000
      else UNDEFINED
      endif endif
   else UNDEFINED
   endif
  ANNUAL
