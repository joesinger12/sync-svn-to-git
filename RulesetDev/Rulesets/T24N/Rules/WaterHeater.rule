  DATATYPE
    Float
  LONGFORM
    BaselineHeatPumpWaterHeaterCOP
  DESCRIPTION
    "Baseline heat pump water heater COP"
  INPUTCLASS
    NotInput
  DEFAULT
    1.94778 + ( 1556.213333 / ( 3412 * 4.5 ) )
