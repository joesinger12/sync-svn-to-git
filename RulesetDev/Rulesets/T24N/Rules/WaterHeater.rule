      InpPwr * COP * 3412 * CmbdMult                // Heat Pump
    else if( Type = "Conventional" .AND. IfValidAnd( CapRtd > 0 ) )
    then
      CapRtd * IfValidAnd( ThrmlEff > 0 ) * CmbdMult
    else UNDEFINED
    endif endif
  ANNUAL
    z:HtOutput
