              UNDEFINED
            case 60:   // Consumer Instantaneous Electric Water Heater
              RE
            case 70:  // Residential-Duty Commercial Electric Instantaneous Water Heater
              UNDEFINED
            default:
              UNDEFINED
          endswitch
        else UNDEFINED          
        endif endif
      else if( Proj:SHWBothBaseline )
