    if( Type <> "Conventional" )
    then 
      if( u:Type = "HeatPumpSplit" .AND. IfValidAnd( InpPwr > 0 ) )
      then
        InpPwr * 0.85 * 1000
      else if( u:Type = "HeatPumpPackaged" .AND. IfValidAnd( InpPwr > 0 ) )
      then
