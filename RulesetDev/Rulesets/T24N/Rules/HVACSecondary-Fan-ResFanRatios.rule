// Calculate ratio of exhuast fan power to total system power
RULE NEW AirSys:ExhFanPwrRat
  DATATYPE
    Float
  LONGFORM
    ExhaustFanPowerRatio
  INPUTCLASS
    NotInput
  ANNUAL_PROPOSED
    if( ServesResZn > 0 ) 
    then 0
    else UNCHANGED
    endif
;    if( TotPropSysFanPwr > 0 ) 
;    then
;      SumRevRef( ResOtherZn:ExhSysRef, ResOtherZn:PropExhFanPwr ) /
