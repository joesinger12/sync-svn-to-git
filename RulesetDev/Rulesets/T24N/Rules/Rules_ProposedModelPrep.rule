               then  1  else  UNDEFINED  endif
         else if (EnumValue( Type ) >= 10 .AND. LocalStatus( Key[1] ) > 0)
         then  if (Key[1] == Key[ 2] .AND. Key[1] == Key[ 3] .AND. Key[1] == Key[ 4] .AND. Key[1] == Key[ 5] .AND. Key[1] == Key[ 6] .AND.
