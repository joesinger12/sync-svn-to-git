;*************************************************************************
RULELIST "ProposedModelPrep" 1 0 1 1
;*************************************************************************

   ;--------------------------------------------------------
   ; Setup Central HPWH ResDHWSys objects for simulation
   ;--------------------------------------------------------
;"dbg" Proj:Action = { PostMessageToLog( "   evaluating 'ProposedModelPrep' rules..." ) }
; RES DHW REMOVAL
;   "Setup MFam Central HPWH Res DHW system(s)"  ResDHWSys:Action  =     ; SAC 1/13/20 (Com tic #3156)
;      {  if (IfValidAnd( CentralSysTypeVal == 2 )) then  EvalRulelist( "PostProposedInput_CentralHPWH", 1 )  else  UNCHANGED  endif  }
;   "Redefault Prop DHW stuff if systems modified for CHPWH"    Proj:Action  =    ; SAC 1/5/20 (tic #862)
;      {  if (SumAcrossIf( ResDHWSys:SysFlrArea, ResDHWSys:CentralSysTypeVal = 2 ) > 0) then  EvalRulelist( "rl_DEFAULT" )  else  UNCHANGED  endif  }

   ;--------------------------------------------------------
   ; Create/Assign Discharge, Charge & ChillerOnly On/Off schedules for ThrmlEngyStor based on ModeSchRef
   ;--------------------------------------------------------
   "Create ThrmlEngyStorModeSchDay:DischrgSchDayRef if necessary"    ThrmlEngyStorModeSchDay:Action  = {
         if (LocalCompAssigned( DischrgSchDayRef )) then  UNCHANGED
         else  EvalRulelist( "Create_ThrmlEngyStorModeSchDay_DischrgSchDayRef", 1 )  endif  }
   "Create ThrmlEngyStorModeSchDay:ChrgSchDayRef if necessary"       ThrmlEngyStorModeSchDay:Action  = {
         if (LocalCompAssigned( ChrgSchDayRef )) then  UNCHANGED
         else  EvalRulelist( "Create_ThrmlEngyStorModeSchDay_ChrgSchDayRef", 1 )  endif  }
   "Create ThrmlEngyStorModeSchDay:ChlrOnlySchDayRef if necessary"   ThrmlEngyStorModeSchDay:Action  = {
         if (LocalCompAssigned( ChlrOnlySchDayRef )) then  UNCHANGED
         else  EvalRulelist( "Create_ThrmlEngyStorModeSchDay_ChlrOnlySchDayRef", 1 )  endif  }

   "Create SchWeek:DischrgSchWeekRef if necessary"    SchWeek:Action  = {
         if (Type != "ThrmlEngyStorMode" .OR. LocalCompAssigned( DischrgSchWeekRef  )) then  UNCHANGED
         else  EvalRulelist( "Create_SchWeek_DischrgSchWeekRef", 1 )   endif  }
   "Create SchWeek:ChrgSchWeekRef if necessary"       SchWeek:Action  = {
         if (Type != "ThrmlEngyStorMode" .OR. LocalCompAssigned( ChrgSchWeekRef     )) then  UNCHANGED
         else  EvalRulelist( "Create_SchWeek_ChrgSchWeekRef", 1 )      endif  }
   "Create SchWeek:ChlrOnlySchWeekRef if necessary"   SchWeek:Action  = {
         if (Type != "ThrmlEngyStorMode" .OR. LocalCompAssigned( ChlrOnlySchWeekRef )) then  UNCHANGED
         else  EvalRulelist( "Create_SchWeek_ChlrOnlySchWeekRef", 1 )  endif  }

   "Create Sch:DischrgSchRef if necessary"    Sch:Action  = {
         if (Type != "ThrmlEngyStorMode" .OR. LocalCompAssigned( DischrgSchRef  )) then  UNCHANGED
         else  EvalRulelist( "Create_Sch_DischrgSchRef", 1 )    endif  }
   "Create Sch:ChrgSchRef if necessary"       Sch:Action  = {
         if (Type != "ThrmlEngyStorMode" .OR. LocalCompAssigned( ChrgSchRef  )) then  UNCHANGED
         else  EvalRulelist( "Create_Sch_ChrgSchRef", 1 )       endif  }
   "Create Sch:ChlrOnlySchRef if necessary"   Sch:Action  = {
         if (Type != "ThrmlEngyStorMode" .OR. LocalCompAssigned( ChlrOnlySchRef  )) then  UNCHANGED
         else  EvalRulelist( "Create_Sch_ChlrOnlySchRef", 1 )   endif  }

   "Set ThrmlEngyStor:DischrgSchRef"      ThrmlEngyStor:DischrgSchRef   = {
         if (LocalCompAssigned( ModeSchRef:DischrgSchRef  )==0) then  UNCHANGED
         else  ModeSchRef:DischrgSchRef   endif  }
   "Set ThrmlEngyStor:ChrgSchRef"         ThrmlEngyStor:ChrgSchRef      = {
         if (LocalCompAssigned( ModeSchRef:ChrgSchRef     )==0) then  UNCHANGED
         else  ModeSchRef:ChrgSchRef      endif  }
   "Set ThrmlEngyStor:ChlrOnlySchRef"     ThrmlEngyStor:ChlrOnlySchRef  = {
         if (LocalCompAssigned( ModeSchRef:ChlrOnlySchRef )==0) then  UNCHANGED
         else  ModeSchRef:ChlrOnlySchRef  endif  }

  ; now that all TES Mode schedules have been converted to OnOff and assigned for OSM/E+, BLAST the TES Mode schedules
   "Delete TES Mode Sch objects"          Sch:Action   = {
         if (Type != "ThrmlEngyStorMode") then  UNCHANGED
         else  DeleteComp()    endif  }
   "Delete TES Mode SchWeek objects"      SchWeek:Action   = {
         if (Type != "ThrmlEngyStorMode") then  UNCHANGED
         else  DeleteComp()    endif  }
   "Delete all ThrmlEngyStorModeSchDay objects"   ThrmlEngyStorModeSchDay:Action   = {
         DeleteComp()  }
         
   "Eval E+ sim prep rules" Proj:Action = 
      { EvalRulelist( "E+_SimulationPrep_Res" ) }

   ;--------------------------------------------------------------------
   ; All PostProposedInput DHW - Demand Response rules
   ;--------------------------------------------------------------------
   "Setup ResDHWSys demand response schedules"           ResDHWSys:Action  =     ; SAC 12/04/20
      {  if (IfValidAnd( DemRespValid > 0 )==0) then  UNCHANGED
         else  EvalRulelist( "SetupDemRespSched_ResDHWSys", 1 )  endif  }

   ; setup of ResOtherZn nonres DHW properties - SAC 02/22/23 (tic #3418)
   "set/import ResOtherZn:HotWtrHtgSchRef"      ResOtherZn:HotWtrHtgSchRef =
      {  if (IfValidAnd( HotWtrHrlyUsePerPers > 0 ) .AND.
             EnumValue( FuncSchGrp ) > 0)
         then  RuleLibrary(Schedule,(SpaceFunctionGroups:HotWtrHtgSchRef("FuncGroup",FuncSchGrp)))
         else  UNCHANGED  endif  }
   "set ResOtherZn:HotWtrSupTemp"               ResOtherZn:HotWtrSupTemp =
      {  if (LocalCompAssigned( HotWtrHtgSchRef ))
         then  135
         else  UNDEFINED  endif  }
;           "HotWtrSupTempSchRef",  BEMP_Obj,  1,  0,  1,  Pres,  "",              0,  1, "Schedule",  3, "Type",   3008, "HotWaterSupplyTemperatureScheduleReference",  ""  

   "Default all Sch objects"   Proj:Action  = {  EvalRulelist( "Default_Schedules" )  }   ; SAC 08/26/21

   ;--------------------------------------------------------
   ; ... next OSM/E+ prep ...
   ;--------------------------------------------------------




; CANNOT DO NORMAL MODEL DEFAULTING, AS THESE RULES ARE MID-ANALYSIS SO USER DEFAULTING COULD FOWL UP PROPOSED/STANDARD SETTINGS
;   ;--------------------------------------------------------
;   ; Perform normal model defaulting before returning
;   ;--------------------------------------------------------
;   "Perform normal defaulting"   Proj:Action  = {  EvalRulelist( "rl_DEFAULT" )  }

END
;*************************************************************************


;*************************************************************************
RULELIST "Create_Sch_DischrgSchRef" 1 0 1 1
;*************************************************************************
   "Create Sch:DischrgSchRef"          Sch:DischrgSchRef  = {
         CreateComp( Sch, Name + "-dischrg" )  }
   "Set TES Mode DischrgSchRef:Type"   Sch:DischrgSchRef:Type    = {  "OnOff"  }
   "Set TES Mode DischrgSchRef:Parent" Sch:DischrgSchRef:Parent  = {  Proj:Name  }

  ; first month/day/week sched
   "Set TES Mode Sch:EndMonth[1]"      Sch:DischrgSchRef:EndMonth[1] = {
         if (NumWeeks < 1) then  UNDEFINED
         else  EndMonth[1]   endif  }
   "Set TES Mode Sch:EndDay[1]"        Sch:DischrgSchRef:EndDay[1] = {
         if (NumWeeks < 1) then  UNDEFINED
         else  EndDay[1]     endif  }
   "Set TES Mode Sch:SchWeekRef[1]"    Sch:DischrgSchRef:SchWeekRef[1] = {
         if (NumWeeks < 1) then  UNDEFINED
         else  SchWeekRef[1]:DischrgSchWeekRef   endif  }

  ; second month/day/week sched
   "Set TES Mode Sch:EndMonth[2]"      Sch:DischrgSchRef:EndMonth[2] = {
         if (NumWeeks < 2) then  UNDEFINED
         else  EndMonth[2]   endif  }
   "Set TES Mode Sch:EndDay[2]"        Sch:DischrgSchRef:EndDay[2] = {
         if (NumWeeks < 2) then  UNDEFINED
         else  EndDay[2]     endif  }
   "Set TES Mode Sch:SchWeekRef[2]"    Sch:DischrgSchRef:SchWeekRef[2] = {
         if (NumWeeks < 2) then  UNDEFINED
         else  SchWeekRef[2]:DischrgSchWeekRef   endif  }

  ; third month/day/week sched
   "Set TES Mode Sch:EndMonth[3]"      Sch:DischrgSchRef:EndMonth[3] = {
         if (NumWeeks < 3) then  UNDEFINED
         else  EndMonth[3]   endif  }
   "Set TES Mode Sch:EndDay[3]"        Sch:DischrgSchRef:EndDay[3] = {
         if (NumWeeks < 3) then  UNDEFINED
         else  EndDay[3]     endif  }
   "Set TES Mode Sch:SchWeekRef[3]"    Sch:DischrgSchRef:SchWeekRef[3] = {
         if (NumWeeks < 3) then  UNDEFINED
         else  SchWeekRef[3]:DischrgSchWeekRef   endif  }

  ; additional month/day/week sched setting via other rulelists if needed
   "Setup Sch:DischrgSchRef[4-12] if necessary"    Sch:Action   = {
         if (IfValidAnd( NumWeeks < 4 )) then  UNCHANGED
         else  EvalRulelist( "Setup_Sch_DischrgSchRef_WksThru12", 1 )    endif  }
   "Setup Sch:DischrgSchRef[13-] if necessary"     Sch:Action   = {
         if (IfValidAnd( NumWeeks < 13 )) then  UNCHANGED
         else  EvalRulelist( "Setup_Sch_DischrgSchRef_Wks13On", 1 )    endif  }
END

