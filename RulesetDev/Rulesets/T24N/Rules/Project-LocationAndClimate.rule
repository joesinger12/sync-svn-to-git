// -------------- Get coldest-month wind speed from Weather Table --------------------------
RULE NEW Proj:WindSpeedColdestMonth
  DESCRIPTION
    "set the correct column lookup for winter DD wind speed"
  HELP
    "Locates and sets coldest month for monthly average wind speed lookup"
  INPUTCLASS
    NotInput
  UNITS
    MPH
  DEFAULT
    if (IfValidAnd(WeatherStationNum > 0 ) .AND. WeatherStationValid )
    then
      if ( ColdMonth = 11 )
      then
        WeatherCliZnDsgnDay2022:WSAvg_Nov("Station_Name", u:WeatherStation ) 
      else if ( ColdMonth = 12 )
      then
        WeatherCliZnDsgnDay2022:WSAvg_Dec("Station_Name", u:WeatherStation )
      else if ( ColdMonth = 1 )
      then
        WeatherCliZnDsgnDay2022:WSAvg_Jan("Station_Name", u:WeatherStation )
      else if ( ColdMonth = 2 )
      then
        WeatherCliZnDsgnDay2022:WSAvg_Feb("Station_Name", u:WeatherStation )
      else if ( ColdMonth = 3 )
      then
        WeatherCliZnDsgnDay2022:WSAvg_Mar("Station_Name", u:WeatherStation )
      else 
        UNDEFINED
      endif endif endif endif endif
    else
      UNDEFINED
    endif
ENDRULE

