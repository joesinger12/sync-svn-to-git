RULE Proj:WeatherStationNum
  DESCRIPTION
    "California weather station number."
  INPUTCLASS
    Optional
  DEFAULT
    if( LocalStatus( WeatherStation ) > 4 .AND. WeatherStationValid ) 
    then // If weather station is user-defined, then station # is based on that
        WeatherCliZnDsgnDay2022:WMO( "Station_Name", u:WeatherStation )
    else if (IfValidAnd( AnalysisVersion > 250000 ) .AND. IfValidAnd( CliZnNum > 0 ))
    then  switch (CliZnNum)
             case  1  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "CALIFORNIA REDWOOD COAST" )
             case  2  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "SONOMA COUNTY AP"         )
             case  3  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "OAKLAND INTL"             )
             case  4  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "PASO ROBLES"              )
             case  5  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "SANTA MARIA"              )
             case  6  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "LOS ANGELES INTL"         )
             case  7  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "SAN DIEGO INTL"           )
             case  8  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "FULLERTON"                )
             case  9  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "HOLLYWOOD BURBANK"        )
             case 10  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "RIVERSIDE"                )
             case 11  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "RED BLUFF"                )
             case 12  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "SACRAMENTO EXECUTIVE"     )
             case 13  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "FRESNO YOSEMITE"          )
             case 14  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "PALMDALE"                 )
             case 15  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "PALM SPRINGS"             )
             case 16  :  WeatherCliZnDsgnDay2022:WMO( "Station_Name", "BLUE CANYON"              )
             default  :  0
          endswitch
    else if( LocalStatus( ZipCode ) = 0 .OR. LocalStatus( CliZnNum ) = 0 )
    then // Weather station number set to 0 if ZipCode or CliZnNum not found
      0
    else if( ClimateZoneBoundaries:IsValid( "ZipCode", u:ZipCode, "CliZn", u:CliZnNum) > 0 )
    then // ZipCode/CliZnNum is valid, set station # based on these inputs
      ClimateZoneBoundaries:WthrStationNum( "ZipCode", u:ZipCode, "CliZn", u:CliZnNum ) 
    else 0
    endif endif endif endif
  CHECKCODE
    if( WeatherStationNum > 0 )
    then UNCHANGED 
    else PostError( "Invalid weather station number (%.0f)", WeatherStationNum )
    endif
  SIZING  
    u:WeatherStationNum
  ANNUAL
    u:WeatherStationNum
ENDRULE

RULE Proj:WeatherStation
  DESCRIPTION
