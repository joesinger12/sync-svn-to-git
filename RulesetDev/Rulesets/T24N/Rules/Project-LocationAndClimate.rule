// ********** Latitude and Longitude *******************************************
RULE Proj:Lat
  DESCRIPTION
    "Building latitude."
  INPUTCLASS
    Required
  MINIMUM
    -90
  MAXIMUM
     90
  DEFAULT
    if( LocalStatus( ZipCode ) > 0 )
    then
      if( ZipCode > 90000 .AND. ZipCode < 99999 )
  	   then ClimateZoneBoundaries:Lat( "ZipCode", u:ZipCode )
      else if( WeatherStationNum > 0 ) 
  	   then WeatherCliZnDsgnDay2022:Lat( "WMO", u:WeatherStationNum )
  	   else UNDEFINED
      endif endif
    else if( IfValidAnd( WeatherStationNum > 0 )==0 )
    then UNDEFINED
    else WeatherCliZnDsgnDay2022:Lat( "WMO", u:WeatherStationNum )
    endif endif
  CHECKCODE
    if( Lat >= 32.4 .AND. Lat <= 42 ) 
    then UNCHANGED
    else PostWarning( "Invalid CA project latitude (%f, valid range 32.4-42.0)", Lat )
    endif
  SIZING
    u:Lat
  ANNUAL
    u:Lat
ENDRULE

RULE Proj:Long
  DESCRIPTION
    "Building longitude."
  INPUTCLASS
    Required
  MINIMUM
    -180
  MAXIMUM
     180
  DEFAULT
    if( LocalStatus( ZipCode ) > 0 )
  	then
      if( ZipCode > 90000 .AND. ZipCode < 99999)
      then ClimateZoneBoundaries:Long( "ZipCode", u:ZipCode )
      else if( WeatherStationNum > 0) 
      then WeatherCliZnDsgnDay2022:Lon( "WMO", u:WeatherStationNum )
      else  UNDEFINED
      endif endif
    else if( IfValidAnd( WeatherStationNum > 0 )==0 )
    then UNDEFINED
	  else WeatherCliZnDsgnDay2022:Lon( "WMO", u:WeatherStationNum )
    endif endif
  CHECKCODE
    if( Long >= -124.5 .AND. Long <= -113.5)
    then UNCHANGED
    else PostWarning( "Invalid CA project longitude (%f, valid range -124.5 - -113.5)", Long )
    endif
  SIZING
    u:Long
  ANNUAL
    u:Long
ENDRULE

RULE Proj:Elevation
  DESCRIPTION
    "Building elevation."
  INPUTCLASS
    Prescribed
  MINIMUM
    -300
  MAXIMUM
    8899
  DEFAULT 
    if( IfValidAnd( WeatherStationNum > 0 )==0 )
    then UNDEFINED
    else WeatherCliZnDsgnDay2022:Elev( "WMO", u:WeatherStationNum )
    endif
  SIZING
    u:Elevation
  ANNUAL
    u:Elevation
ENDRULE


