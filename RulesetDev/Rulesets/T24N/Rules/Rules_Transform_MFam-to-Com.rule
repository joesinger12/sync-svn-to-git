RULELIST  "Transform_ResMat_to_Mat_Properties" 1 0 1 0
;   Rules to transform a fully defaulted ResMat object
;   to (Com) Mat for simulation in E+
;*************************************************************************

;   Mat:CodeCat  should already be set to  "Specify Properties"

   "Set Mat:UserRVal"          Mat:UserRVal  =
      {  if (IfValidAnd( MatchingResMatRef:Thickness   > 0 ) .AND.
             IfValidAnd( MatchingResMatRef:RValPerInch > 0 ))
         then  MatchingResMatRef:Thickness * MatchingResMatRef:RValPerInch
         else if (IfValidAnd( MatchingResMatRef:Resistance > 0 ))
         then  MatchingResMatRef:Resistance
         else  UNCHANGED  endif endif  }

   "Set Mat:UserThrmlCndct"    Mat:UserThrmlCndct  =
      {  if (IfValidAnd( MatchingResMatRef:Conductivity > 0 ))
         then  MatchingResMatRef:Conductivity
         else  UNCHANGED  endif  }

   "Set Mat:UserSpecHt"       Mat:UserSpecHt  =
      {  if (IfValidAnd( MatchingResMatRef:SpecHeat > 0 ))
         then  MatchingResMatRef:SpecHeat
         else  UNCHANGED  endif  }

   "Set Mat:UserDens"         Mat:UserDens  =
      {  if (IfValidAnd( MatchingResMatRef:Density > 0 ))
         then  MatchingResMatRef:Density
         else  UNCHANGED  endif  }

   "Set Mat:UserThkns"        Mat:UserThkns  =
      {  if (IfValidAnd( MatchingResMatRef:Thickness > 0 ))
         then  MatchingResMatRef:Thickness
         else  UNCHANGED  endif  }


