;                                                                                       (FurringInsul2Layer == 900 .AND. IfValidAnd( FurringInsul2LayerRVal > 0 )==0))) then  0
;																									else if (InsideFinishLayer > 0.1) then  2
;																									else  1  endif endif  }
;	"Set Cons:FrameCavityLayerMat[2]"	Cons:FrameCavityLayerMat[2]	= {	if (FurringLayer < 1 .AND. (FurringInsulLayer < 1 .OR. FurringInsulLayer == 100401 .OR.
;                                                                                      (FurringInsulLayer == 900 .AND. IfValidAnd( FurringInsulLayerRVal > 0 )==0))) then  0
;																									else if (InsideFinishLayer > 0.1 .AND. FrameCavityLayerMat[1] > 0) then  4
;																									else if (InsideFinishLayer > 0.1 .OR.  FrameCavityLayerMat[1] > 0) then  3
;																									else  2  endif endif endif  }



; RJH experimentation starts HERE

   "Set Mat layer FrmMat for Composite CodeCat materials"  for:#Lyr=(1to10)
      ResConsAssm:MatchingConsAssmRef:MatRef[#Lyr]:FrmMat  =
         {  if (MatchingConsAssmRef:MatRef[#Lyr]:CodeCat == 40) 
            then switch (EnumString(Type)) 
                    case "Wood Framed Wall"    : "Wood"
                    case "Wood Framed Ceiling" : "Wood"
                    case "Wood Framed Floor"   : "Wood"
                    case "Steel Framed Wall"   : "Steel"
                    default :  UNCHANGED
                 endswitch
            else  UNCHANGED  
            endif  }

; MFAM TO DO - deal w/ SIPS walls/etc.

   "Set Mat layer FrmConfig for Composite CodeCat materials"  for:#Lyr=(1to10)
      ResConsAssm:MatchingConsAssmRef:MatRef[#Lyr]:FrmConfig  =
         {  if (MatchingConsAssmRef:MatRef[#Lyr]:CodeCat == 40) 
            then if (EnumString(CanAssignTo) == "Exterior Walls" .OR. EnumString(CanAssignTo) == "Interior Walls" .OR. EnumString(CanAssignTo) == "Underground Walls") 
                 then switch (EnumString(FrameLayer)) 
                        case "2x4 AWF"            : "WallAWS24inOC"
                        case "2x6 AWF"            : "WallAWS24inOC"
                        default : switch (mod( FrameLayer, 100 ))
                                    case 16 : "Wall16inOC"
                                    case 24 : "Wall24inOC"
                                    default : UNCHANGED
                                  endswitch
                      endswitch
                 else if (EnumString(CanAssignTo) == "Attic Roofs" .OR. EnumString(CanAssignTo) == "Cathedral Ceilings" .OR. EnumString(CanAssignTo) == "Ceilings (below attic)" .OR. EnumString(CanAssignTo) == "Interior Ceilings")
                 then switch (EnumString(FrameLayer)) 
                        case "2x4 Bottom Chord of Truss @ 24 in. O.C."   : "Roof24inOC"
                        case "2x4 Top Chord of Roof Truss @ 24 in. O.C." : "Roof24inOC"
                        default : switch (mod( FrameLayer, 100 ))
                                    case 16 : "Roof16inOC"
                                    case 24 : "Roof24inOC"
                                    default : UNCHANGED
                                  endswitch
                      endswitch
                 else if (EnumString(CanAssignTo) == "Interior Floors" .OR. EnumString(CanAssignTo) == "Exterior Floors" .OR. EnumString(CanAssignTo) == "Floors Over Crawlspace")
                 then switch (mod( FrameLayer, 100 )) 
                        case 16 : "Floor16inOC"
                        case 24 : "Floor24inOC"
                        default :  UNCHANGED
                      endswitch
                 else UNCHANGED
                 endif endif endif
            else UNCHANGED  
            endif  }

   "Set Mat layer FrmDepth for Composite CodeCat materials"  for:#Lyr=(1to10)
      ResConsAssm:MatchingConsAssmRef:MatRef[#Lyr]:FrmDepth  =
         {  if (MatchingConsAssmRef:MatRef[#Lyr]:CodeCat == 40) 
            then switch (EnumString(FrameLayer)) 
                    case "2x4 AWF" : "3_5In"
                    case "2x6 AWF" : "5_5In"
                    case "2x4 Bottom Chord of Truss @ 24 in. O.C."   : "3_5In"
                    case "2x4 Top Chord of Roof Truss @ 24 in. O.C." : "3_5In"
                    default : switch (int( FrameLayer / 100 ))
                                case  4 : "3_5In"
                                case  6 : "5_5In"
                                case  8 : "7_25In"
                                case 10 : "9_25In"
                                case 12 : "11_25In"
                                default : UNCHANGED
                                endswitch
                 endswitch
            else UNCHANGED  
            endif  }

   "Set Mat layer CavityIns for Composite CodeCat materials"  for:#Lyr=(1to10)
      ResConsAssm:MatchingConsAssmRef:MatRef[#Lyr]:CavityIns  =
         {  if (MatchingConsAssmRef:MatRef[#Lyr]:CodeCat == 40) 
            then if (IfValidAnd( CavityLayerRVal > 0 ))
                 then CavityLayerRVal
                 else UNCHANGED
                 endif
            else UNCHANGED  
            endif  }




