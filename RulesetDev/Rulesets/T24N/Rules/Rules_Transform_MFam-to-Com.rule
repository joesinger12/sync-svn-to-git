;*************************************************************************
RULELIST  "Transform_ResConsAssm_to_ConsAssm" 1 0 1 0
;   Rules to transform a fully defaulted ResConsAssm object (and its assigned ResMats)
;   to (layer-by-layer) ConsAssm (and subordinate Mats) for simulation in E+
;*************************************************************************

"dbg" ResConsAssm:Action = { PostMessageToLog( "   processing 'Transform_ResConsAssm_to_ConsAssm' on ResConsAssm '%s'", Name ) }

   "Create ConsAssm matching this ResConsAssm"              ResConsAssm:MatchingConsAssmRef  =
      {  CreateComp( ConsAssm, Name + "-com" )  }
   "Set cross-reference from ConsAssm to this ResConsAssm"  ResConsAssm:MatchingConsAssmRef:MatchingResConsAssmRef  = {  Name  }

   ;"Create Mat layer consistent w/ ResConsAssm:(Cavity/Frame)PathMats[*]"  for:#Lyr=(1to10)
   ;   ResConsAssm:CavityPathMats[#Lyr]:MatchingMatRef  =
   "Create Mat layer consistent w/ ResConsAssm:(Cavity/Frame)PathMats[*]"  for:#Lyr=(1to10)
      ResConsAssm:MatchingConsAssmRef:MatRef[#Lyr]  =
         {  if (NumLayerMats < #Lyr .OR. LocalCompAssigned( CavityPathMats[#Lyr]:MatchingMatRef ) > 0)
            then  UNCHANGED   ; non-existent layer or already transformed
            else if ( ( IfValidAnd( NumPaths = 1 ) .AND. LocalCompAssigned( CavityPathMats[#Lyr] ) .AND.
                        IfValidAnd( FrameCavityLayerMat[1] = #Lyr )==0 .AND.
                        IfValidAnd( FrameCavityLayerMat[2] = #Lyr )==0 ) .OR.
                      ( IfValidAnd( NumPaths = 2 ) .AND. IfValidAnd( CavityPathMats[#Lyr] = FramePathMats[#Lyr] ) ) )            
            then  ; DEFINE MAT BASED ON CavityPathMats[#Lyr]
                  if (IfValidAnd( CavityPathMats[#Lyr]:Thickness > 0 ))
                  then  if (IfValidAnd( CavityPathMats[#Lyr]:RValPerInch > 0 ))
                        then  AssignOrCreateComp( Mat, CavityPathMats[#Lyr]:Name + "-com",
                                 "CodeCat", "Specify Properties",  "UserRVal", (CavityPathMats[#Lyr]:Thickness * CavityPathMats[#Lyr]:RValPerInch),
											"UserThrmlCndct", CavityPathMats[#Lyr]:Conductivity, "UserSpecHt", CavityPathMats[#Lyr]:SpecHeat,
                                 "UserDens",       CavityPathMats[#Lyr]:Density,      "UserThkns",  CavityPathMats[#Lyr]:Thickness )
                        else if (IfValidAnd( CavityPathMats[#Lyr]:Resistance > 0 ))
                        then  AssignOrCreateComp( Mat, CavityPathMats[#Lyr]:Name + "-com",
                                 "CodeCat", "Specify Properties",  "UserRVal", CavityPathMats[#Lyr]:Resistance,
											"UserThrmlCndct", CavityPathMats[#Lyr]:Conductivity, "UserSpecHt", CavityPathMats[#Lyr]:SpecHeat,
                                 "UserDens",       CavityPathMats[#Lyr]:Density,      "UserThkns",  CavityPathMats[#Lyr]:Thickness )
                        else  AssignOrCreateComp( Mat, CavityPathMats[#Lyr]:Name + "-com",
                                 "CodeCat", "Specify Properties",
											"UserThrmlCndct", CavityPathMats[#Lyr]:Conductivity, "UserSpecHt", CavityPathMats[#Lyr]:SpecHeat,
                                 "UserDens",       CavityPathMats[#Lyr]:Density,      "UserThkns",  CavityPathMats[#Lyr]:Thickness )   endif endif
                  else if (IfValidAnd( Thickness[#Lyr] > 0 ))
                  then  if (IfValidAnd( CavityPathMats[#Lyr]:RValPerInch > 0 ))
                        then  AssignOrCreateComp( Mat, Format( "%s-thck%sin-com", CavityPathMats[#Lyr]:Name, FltToStr( Thickness[#Lyr], 1, 1, 0, 1 ) ),
                                 "CodeCat", "Specify Properties",  "UserRVal", (Thickness[#Lyr] * CavityPathMats[#Lyr]:RValPerInch),
											"UserThrmlCndct", CavityPathMats[#Lyr]:Conductivity, "UserSpecHt", CavityPathMats[#Lyr]:SpecHeat,
                                 "UserDens",       CavityPathMats[#Lyr]:Density,      "UserThkns",  Thickness[#Lyr] )
                        else if (IfValidAnd( CavityPathMats[#Lyr]:Resistance > 0 ))
                        then  AssignOrCreateComp( Mat, Format( "%s-thck%sin-com", CavityPathMats[#Lyr]:Name, FltToStr( Thickness[#Lyr], 1, 1, 0, 1 ) ),
                                 "CodeCat", "Specify Properties",  "UserRVal", CavityPathMats[#Lyr]:Resistance,
											"UserThrmlCndct", CavityPathMats[#Lyr]:Conductivity, "UserSpecHt", CavityPathMats[#Lyr]:SpecHeat,
                                 "UserDens",       CavityPathMats[#Lyr]:Density,      "UserThkns",  Thickness[#Lyr] )
                        else  AssignOrCreateComp( Mat, Format( "%s-thck%sin-com", CavityPathMats[#Lyr]:Name, FltToStr( Thickness[#Lyr], 1, 1, 0, 1 ) ),
                                 "CodeCat", "Specify Properties",
											"UserThrmlCndct", CavityPathMats[#Lyr]:Conductivity, "UserSpecHt", CavityPathMats[#Lyr]:SpecHeat,
                                 "UserDens",       CavityPathMats[#Lyr]:Density,      "UserThkns",  Thickness[#Lyr] )   endif endif
                  else  UNCHANGED  ; error - no thickness defined, either in the ResMat or ResConsAssm[layer]
                  endif endif
            else if (IfValidAnd( NumPaths = 2 ) .AND. IfValidAnd( FrameCavityLayerMat[1] = #Lyr ))
            then  CreateComp( Mat, Name + "-cav/frm1-com" )    ; this layer is combo of cavity & frame layer #1
            else if (IfValidAnd( NumPaths = 2 ) .AND. IfValidAnd( FrameCavityLayerMat[2] = #Lyr ))
            then  CreateComp( Mat, Name + "-cav/frm2-com" )    ; this layer is combo of cavity & frame layer #2
            else if (IfValidAnd( NumPaths = 1 ) .AND. IfValidAnd( FrameCavityLayerMat[1] = #Lyr ))
            then  CreateComp( Mat, Name + "-ins/fur1-com" )    ; this layer is combo of insulation & furring layer #1
            else if (IfValidAnd( NumPaths = 1 ) .AND. IfValidAnd( FrameCavityLayerMat[2] = #Lyr ))
            then  CreateComp( Mat, Name + "-ins/fur2-com" )    ; this layer is combo of insulation & furring layer #2
            else  UNCHANGED    ; error??
            endif endif endif endif endif endif  }

   "Set Mat layer ResMatMapping for FrameCavityLayerMat[*]"  for:#Lyr=(1to10)
      ResConsAssm:MatchingConsAssmRef:MatRef[#Lyr]:ResMatMapping  =
         {  if (NumLayerMats < #Lyr) then  UNCHANGED 
            else if (IfValidAnd( NumPaths = 2 ) .AND. IfValidAnd( FrameCavityLayerMat[1] = #Lyr ))
            then  "FrameCavity1"
            else if (IfValidAnd( NumPaths = 2 ) .AND. IfValidAnd( FrameCavityLayerMat[2] = #Lyr ))
            then  "FrameCavity2"
            else if (IfValidAnd( NumPaths = 1 ) .AND. IfValidAnd( FrameCavityLayerMat[1] = #Lyr ))
            then  "FurringInsul1"
            else if (IfValidAnd( NumPaths = 1 ) .AND. IfValidAnd( FrameCavityLayerMat[2] = #Lyr ))
            then  "FurringInsul2"
            else  "Properties"
            endif endif endif endif endif  }

   "Set Mat layer CodeCat for FrameCavityLayerMat[*]"  for:#Lyr=(1to10)
      ResConsAssm:MatchingConsAssmRef:MatRef[#Lyr]:CodeCat  =
         {  switch (EnumString( MatchingConsAssmRef:MatRef[#Lyr]:ResMatMapping ))
               case  "Properties"    :  "Specify Properties"
               case  "FrameCavity1"  :  "Composite"
               case  "FrameCavity2"  :  "Composite"
               case  "FurringInsul1" :  "Composite"      ; ???
               case  "FurringInsul2" :  "Composite"      ; ???
               default  :  UNCHANGED  ; error?
            endswitch  }
; HOW TO HANDLE CONCRETE constructions w/ wood furring??  Like Composite or some other (non-properties) method?


; Concrete Insul/Furring layer
