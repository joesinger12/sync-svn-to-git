RULE ResCentralHtgClgSys:TreeDescrip
  DEFAULT
    if (IfValidAnd( ResProj:IsMultiFamily > 0.5 )==0) then  UNDEFINED
    else if (IfValidAnd( NumAssigningDUs > 0 ))
