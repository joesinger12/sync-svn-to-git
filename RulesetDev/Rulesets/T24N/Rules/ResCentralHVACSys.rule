RULE NEW ResCentralHtgClgSys:TypeAbbrevStr
  DATATYPE
    String
  INPUTCLASS
    NotInput
  DEFAULT
    switch( Type )
      case "Four-pipe fan coil"       : "FPFC"
      case "Water source heat pump"   : "WSHP"
      case "Hydronic heating only"    : "Hydonic Htg Only"
      case "Variable refrigerant flow": "VRF"
      default                         : "unknown"
    endswitch
