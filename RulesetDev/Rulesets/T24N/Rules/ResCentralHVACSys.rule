  INPUTCLASS
    NotInput
  UNITS
    BTU/hr
  SIZING
    if( CentralEqpType = "Variable refrigerant flow" ) 
    then VRFIndoorMaxFanPwr115 * 3.413 + VRFOutdoorMaxClgCapAvil115 * VRFClgEIR * 1.245474063 * 
         ( 0.0 + 0.625 * VRFMaxPLR115 + 0.234375 * VRFMaxPLR115 * VRFMaxPLR115 + 0.087890625 * VRFMaxPLR115 * VRFMaxPLR115 * VRFMaxPLR115 +
           0.032958984 * VRFMaxPLR115 * VRFMaxPLR115 * VRFMaxPLR115 * VRFMaxPLR115 + 0.012359619 * VRFMaxPLR115 * VRFMaxPLR115 * VRFMaxPLR115 * VRFMaxPLR115 * VRFMaxPLR115)
    else UNDEFINED endif
ENDRULE

