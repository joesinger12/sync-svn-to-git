RULE ResHVACSys:CentralSysClgCap
// convert net capacity to gross capacity ( net capacity + fan heat )
  SIZING
    if( CentralEqpType = "Variable refrigerant flow" ) 
    then CentralSysClgCap + ( CentralSysClgCap / ( 1 - Proj:FanHtCoeff ) ) * Proj:FanHtCoeff else UNCHANGED endif
ENDRULE

RULE ResHVACSys:CentralSysHtgCap
// convert net capacity to gross capacity ( net capacity - fan heat )
  SIZING
    if( CentralEqpType = "Variable refrigerant flow" ) 
    then CentralSysHtgCap - ( CentralSysClgCap / ( 1 - Proj:FanHtCoeff ) ) * Proj:FanHtCoeff else UNCHANGED endif
ENDRULE

