    else UNDEFINED
    endif
  ANNUAL
    if( IfValidAnd( MtrBHPStart > 0 ) .AND.
        IfValidAnd( MtrEffStart > 0 ) )
    then MtrBHPStart * MtrEffStart
    else UNCHANGED
    endif
ENDRULE
RULE NEW Fan:MtrHPIdxGuess1
