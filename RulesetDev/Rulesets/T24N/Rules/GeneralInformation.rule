// ---------- Section 5.2.1 - General Information ------------------------------
// ********** Project Name *****************************************************
RULE Project:Name
  DESCRIPTION
    "Name used for the project, if one is applicable"  
  HELP
    ""  
  REFERENCE 
    ACM Section 5.2.1-General Information
  INPUTCLASS
    Required
ENDRULE

// ********** Project Address **************************************************
// -----------------------------------------------------------------------------
RULE Proj:StAddress
  DESCRIPTION
    "Street address where the project is located"
  HELP
    ""  
  REFERENCE 
    ACM Section 5.2.1-General Information
  INPUTCLASS
    Optional
  DEFAULT
    " - specify - "
ENDRULE

// -----------------------------------------------------------------------------
RULE Proj:City
  DESCRIPTION
    "City where the project is located"
  HELP
    ""  
  REFERENCE 
    ACM Section 5.2.1-General Information
  INPUTCLASS
    Optional
  DEFAULT
    " - specify - "
ENDRULE

// -----------------------------------------------------------------------------
RULE Proj:State
  DESCRIPTION
    "State where the project is located"
  HELP
    ""  
  REFERENCE 
    ACM Section 5.2.1-General Information
  INPUTCLASS
    Optional
  DEFAULT : S901G ECBC
    if( IfValidAnd( ZipCode > 100 ) )
    then // ZipCode is valid, set State based on that
      WeatherFileData_S901G:State( "ZipCode", u:ZipCode )
    else // ZipCode not valid
      if( IfValidAnd( WeatherStationNum > 1000 ) )
      then // WeatherStationNum is valid, set State based on that
        WeatherFileClimateZone_S901G:State( "WthrStationNum", u:WeatherStationNum )
      else // WeatherStationNum also not valid
        UNDEFINED
      endif
    endif
  DEFAULT
    "CA"
ENDRULE

// -----------------------------------------------------------------------------
RULE Proj:ZipCode
  DESCRIPTION
    "Zip Code where the project is located.  Location and Weather File date 
defaulted based on this value."
  HELP
    ""  
  REFERENCE 
    ACM Section 5.2.1-General Information
  INPUTCLASS
    Required
ENDRULE


// ********** Weather Data ***************************************************** SAC 09/06/22
// -----------------------------------------------------------------------------
RULE Proj:WeatherFileSet
  DESCRIPTION
    "Set of weather files to use in analysis"
  INPUTCLASS : T24N_2025
    Prescribed
  INPUTCLASS
    NotInput
  OPTION : T24N_2025
    2025
    2028-SSP2
    2028-SSP3
  OPTION
    n/a
  DEFAULT : T24N_2025
    "2025"
  DEFAULT 
    "n/a" 
ENDRULE		

RULE Proj:WeatherPathSub
  DESCRIPTION
    "Substitutue weather file path (only final portion, same level as default)"
  INPUTCLASS
    Prescribed
  DEFAULT 
    if (IfValidAnd( EnableResearchMode > 0 ) .AND. WeatherFileSet == "2028-SSP2")
    then  "EPW28-SSP2"
    else if (IfValidAnd( EnableResearchMode > 0 ) .AND. WeatherFileSet == "2028-SSP3")
    then  "EPW28-SSP3"
    else  UNDEFINED  endif endif
ENDRULE		

RULE Proj:WeatherStationFileBase
  DESCRIPTION
    "string identifying weather file for the current project location"
  HELP
    ""  
  INPUTCLASS
    Prescribed
  DEFAULT
    if (IfValidAnd( EnableResearchMode > 0 ) .AND. WeatherFileSet == "2028-SSP2" .AND. IfValidAnd( CliZnNum > 0 ))
    then  Format( "%02.0fFSSP2A3", CliZnNum )
    else if (IfValidAnd( EnableResearchMode > 0 ) .AND. WeatherFileSet == "2028-SSP3" .AND. IfValidAnd( CliZnNum > 0 ))
    then  Format( "%02.0fFSSP3A3", CliZnNum )
    else if (IfValidAnd( AnalysisVersion > 250000 ))
    then  switch (WeatherStation)
             case  "CALIFORNIA REDWOOD COAST"   :  "USA_CA_725945_ARCATA-AP_TMY_2000-2020K"
             case  "SONOMA COUNTY AP"           :  "USA_CA_724957_SONOMA-COUNTY-AP_TMY_2000-2020K"
             case  "OAKLAND INTL"               :  "USA_CA_724930_METRO-OAKLAND-INTL_TMY_2000-2020K"
             case  "PASO ROBLES"                :  "USA_CA_723965_PASO-ROBLES-MUNICIPAL_TMY_2000-2020K"
             case  "SANTA MARIA"                :  "USA_CA_723940_SANTA-MARIA-AP_TMY_2000-2020K"
             case  "LOS ANGELES INTL"           :  "USA_CA_722950_LOS-ANGELES-INTL_TMY_2000-2020K"
             case  "SAN DIEGO INTL"             :  "USA_CA_722900_SAN-DIEGO-INTL_TMY_2000-2020K"
             case  "FULLERTON"                  :  "USA_CA_722976_FULLERTON-MUNICIPAL_TMY_2000-2020K"
             case  "HOLLYWOOD BURBANK"          :  "USA_CA_722880_HOLLYWOOD-BURBANK-AP_TMY_2000-2020K"
             case  "RIVERSIDE"                  :  "USA_CA_722869_RIVERSIDE-MUNICIPAL_TMY_2000-2020K"
             case  "RED BLUFF"                  :  "USA_CA_725910_RED-BLUFF-AP_TMY_2000-2020K"
             case  "SACRAMENTO EXECUTIVE"       :  "USA_CA_724830_SACRAMENTO-EXECUTIVE_TMY_2000-2020K"
             case  "FRESNO YOSEMITE"            :  "USA_CA_723890_FRESNO-YOSEMITE-INTL_TMY_2000-2020K"
             case  "PALMDALE"                   :  "USA_CA_723820_PALMDALE-REGIONAL_TMY_2000-2020K"
             case  "PALM SPRINGS"               :  "USA_CA_722868_PALM-SPRINGS-INTL_TMY_2000-2020K"
             case  "BLUE CANYON"                :  "USA_CA_725845_BLUE-CANYON-NYACK-AP_TMY_2000-2020K"
             default   :  UNDEFINED
          endswitch
    else if (IfValidAnd( AnalysisVersion > 220000 ))
    then  if (EnumValue( WeatherStation ) > 0)
          then  WeatherCliZnDsgnDay2022:Weather_FileName( "Station_Name", WeatherStation )
          else  "SACRAMENTO-EXECUTIVE_STYP20"  endif
    else  UNDEFINED  endif endif endif endif
ENDRULE

// -----------------------------------------------------------------------------
RULE Proj:GenerateWeatherLocationDDY
  DESCRIPTION
    "flag indicating whether or not to genreate a DDY file from weather location data"
  HELP
    ""  
  INPUTCLASS
    Prescribed
  DEFAULT
    if (IfValidAnd( AnalysisVersion > 220000 ) .AND. LocalStatus( WeatherStationFileBase ) > 0)
    then  1
    else  UNDEFINED  endif
ENDRULE


