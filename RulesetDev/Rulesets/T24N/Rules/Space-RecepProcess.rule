// -------------- Gas Equipment Power Density --------------------
RULE Spc:GasEqpPwrDens
  DESCRIPTION
    "The use of gas devices represented by a gas equipment annual average 
     power density (Btu/h-ft2) and associated with the occupancy type 
     selected from the Area Category Method or Complete Building Method in 
     ACM Appendix 5.4A."
  INPUTCLASS
    Default
  MINIMUM 
    0
  COMMONMAXIMUM
    20
  MAXIMUM 
    100
  UNITS 
    Btu/hr-ft2
  DEFAULT
    if( HasNoInternalLds )
    then UNDEFINED
    else if( IsProcExhSpc > 0 .AND. HasProcExh = 0 )  
    then UNDEFINED // See issue 1032, comment #10
    else if (IsRes)
    then  UNDEFINED
    else if( LocalCompAssigned(SpcFuncDefaultsRef) .AND. LocalStatus(SpcFunc)<5) 
    then SpcFuncDefaultsRef:GasEqpPwrDens
    else if( Proj:GasType = "None" )
    then 0
    else SpaceFunctionData:GasEqpPwrDens("FuncType",SpcFunc)
    endif endif endif endif endif
  CHECKCODE
    if ( SpcFunc = "Scientific Laboratory Area" .OR. Spc:IsHlthCare .OR.
         SpcFunc = "Laboratory, Scientific" ) ; JP 211130 space type name change (tic #3301)
    then UNCHANGED
    else if( Proj:GasType = "None" .AND. IfValidAnd( GasEqpPwrDens > 0 ) )
	  then PostError("Space '%s' specifies process gas equipment 
	                 but the project gas type is specified as none.  Make the 
	                 process gas equipment load zero or change the project gas 
	                 type.", Name)
	  else if( Proj:GasType != "None" .AND. IfValidAnd( GasEqpPwrDens < SpaceFunctionData:GasEqpPwrDens("FuncType", SpcFunc) ) )
	  then PostError("Space '%s' specifies process gas equipment 
	                 power lower than the prescribed value.  Make the process gas
	                 equipment power equal or greater than the default value.", Name) 
	  else UNCHANGED
    endif endif endif
  SIZING_PROPOSED : S901G ECBC
    if( u:GasEqpPwrDens = 0 .OR.
        LocalStatus(u:GasEqpPwrDens) = 0 )
    then UNDEFINED
    else
      if( HasNoInternalLds )
      then UNDEFINED
      else u:GasEqpPwrDens
      endif
    endif
  SIZING_PROPOSED : T24N_2016
    if( HasNoInternalLds )
    then UNDEFINED
    else if( IsProcExhSpc > 0 .AND. HasProcExh = 0 ) 
    then UNDEFINED // See issue 1032, comment #10
    else SpaceFunctionData:GasEqpPwrDens("FuncType",SpcFunc)
    endif endif
  SIZING_PROPOSED : T24N
    if( HasNoInternalLds )
    then UNDEFINED
    else if( IsProcExhSpc > 0 .AND. HasProcExh = 0 ) 
    then UNDEFINED // See issue 1032, comment #10
    else if (IsRes)
    then  UNDEFINED
    else u:GasEqpPwrDens
    endif endif endif
; ; JP 12/8/22 (tic 3415) This value will be converted to peak value in the end of SIZING_PROPOSED (see PreSim.rule)
  SIZING_BASELINE : S901G ECBC
    if( u:GasEqpPwrDens = 0 .OR.
        LocalStatus(u:GasEqpPwrDens) = 0 )
    then UNDEFINED
    else
      if( GasEqpPwrDensExcptCond > 0 )
      then u:BaseGasEqpPwrDens
      else zp:GasEqpPwrDens
      endif
    endif
  SIZING_BASELINE
    zp:GasEqpPwrDens
  ANNUAL
    z:GasEqpPwrDens
ENDRULE


