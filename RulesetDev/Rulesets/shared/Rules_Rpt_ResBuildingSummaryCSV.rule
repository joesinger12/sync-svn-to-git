	"Check for SpeclFtr:DHWParaPipeCntrl"   	ResProj:SpeclFeatrs:DHWParaPipeCntrl   	= {	if ( SumAll( ResDHWSys:SpeclFtrParaPipeCntrl ) > 0.5 ) then 1 else 0 endif }
	"Check for SpeclFtr:DHWPipeInsul"       	ResProj:SpeclFeatrs:DHWPipeInsul       	= {	if ( SumAll( ResDHWSys:SpeclFtrPipeInsul ) > 0.5 ) then 1 else 0 endif }
	"Check for SpeclFtr:DHWPointOfUse"       	ResProj:SpeclFeatrs:DHWPointOfUse       	= {	if ( SumAll( ResDHWSys:SpeclFtrPointOfUse ) > 0.5 ) then 1 else 0 endif }
	"Check for SpeclFtr:DHWRecircDemOcc"    	ResProj:SpeclFeatrs:DHWRecircDemOcc    	= {	if ( SumAll( ResDHWSys:SpeclFtrRecircDemOcc ) > 0.5 ) then 1 else 0 endif }
	"Check for SpeclFtr:DHWRecircDemPBtn"   	ResProj:SpeclFeatrs:DHWRecircDemPBtn   	= {	if ( SumAll( ResDHWSys:SpeclFtrRecircDemPBtn ) > 0.5 ) then 1 else 0 endif }
	"Check for SpeclFtr:DHWRecircNDemCtrl"  	ResProj:SpeclFeatrs:DHWRecircNDemCtrl  	= {	if ( SumAll( ResDHWSys:SpeclFtrRecircNDemCtrl ) > 0.5 ) then 1 else 0 endif }

	"Write SpeclFtr:DHWMFRecircDemCtrlMsg"        	ResProj:SpeclFeatrs:DHWMFRecircDemCtrlMsg  = {	if ( SpeclFeatrs:DHWMFRecircDemCtrl 	> 0.5 ) then 1 else 0 endif }
	"Write SpeclFtr:DHWMFNLpNRecircMsg"        	ResProj:SpeclFeatrs:DHWMFNLpNRecircMsg     = {	if ( SpeclFeatrs:DHWMFNLpNRecirc	> 0.5 ) then 1 else 0 endif }
	"Write SpeclFtr:DHWMFRecircNDemCtrlMsg"        	ResProj:SpeclFeatrs:DHWMFRecircNDemCtrlMsg = {	if ( SpeclFeatrs:DHWMFRecircNDemCtrl    > 0.5 ) then 1 else 0 endif }
	"Write SpeclFtr:DHWMFRecircTempModMsg"        	ResProj:SpeclFeatrs:DHWMFRecircTempModMsg  = {	if ( SpeclFeatrs:DHWMFRecircTempMod      > 0.5 ) then 1 else 0 endif }
	"Write SpeclFtr:DHWMFRecircTempModMonMsg"       ResProj:SpeclFeatrs:DHWMFRecircTempModMonMsg	= {	if ( SpeclFeatrs:DHWMFRecircTempModMon      > 0.5 ) then 1 else 0 endif }
	"Write SpeclFtr:DHWParaPipeCntrlMsg"    	ResProj:SpeclFeatrs:DHWParaPipeCntrlMsg  	= {	if ( SpeclFeatrs:DHWParaPipeCntrl  	> 0.5 ) then 1 else 0 endif }
	"Write SpeclFtr:DHWPipeInsulMsg"        	ResProj:SpeclFeatrs:DHWPipeInsulMsg      	= {	if ( SpeclFeatrs:DHWPipeInsul      > 0.5 ) then 1 else 0 endif }
	"Write SpeclFtr:DHWPointOfUse"        		ResProj:SpeclFeatrs:DHWPointOfUseMsg      	= {	if ( SpeclFeatrs:DHWPointOfUse      > 0.5 ) then 1 else 0 endif }
	"Write SpeclFtr:DHWRecircDemOccMsg"     	ResProj:SpeclFeatrs:DHWRecircDemOccMsg   	= {	if ( SpeclFeatrs:DHWRecircDemOcc   > 0.5 ) then 1 else 0 endif }
	"Write SpeclFtr:DHWRecircDemPBtnMsg"    	ResProj:SpeclFeatrs:DHWRecircDemPBtnMsg  	= {	if ( SpeclFeatrs:DHWRecircDemPBtn  > 0.5 ) then 1 else 0 endif }
	"Write SpeclFtr:DHWRecircNDemCtrlMsg"   	ResProj:SpeclFeatrs:DHWRecircNDemCtrlMsg 	= {	if ( SpeclFeatrs:DHWRecircNDemCtrl > 0.5 ) then 1 else 0 endif }
; 	"Check for SpeclFtr:DHWParaPipeNCntrl"  	ResProj:SpeclFeatrs:DHWParaPipeNCntrl  	= {	if ( SumAll( ResDHWSys:SpeclFeatrsFlags[3] ) > 0.5 ) then 1 else 0 endif }
; 	"Write SpeclFtr:DHWParaPipeNCntrlMsg"   	ResProj:SpeclFeatrs:DHWParaPipeNCntrlMsg 	= {	if ( SpeclFeatrs:DHWParaPipeNCntrl > 0.5 ) then 1 else 0 endif }

	; SAC 6/2/16 - added reporting of SFamSolarWH (tic #678)   - SAC 2/19/20 - revised logic for new DHWSolarSys assignment (tic #1198)
	"Check for ResDHWSys:SFamSolarWH" 			ResDHWSys:SFamSolarWH					=
	   {  if ( IfValidAnd( ResProj:IsMultiFamily < 0.5 ) .AND. IfValidAnd( FloorAreaServed[1] > 0.1 ) )
         then  if (     IfValidAnd( Proj:AnalysisVersion >= 190102 )    .AND. IfValidAnd( DHWSolarSysRef:ErrID = 0 )) then  2
               else if (IfValidAnd( Proj:AnalysisVersion >= 190102 )==0 .AND. IfValidAnd( SolFracAnnRpt > 0 ) ) then  1
               else  0  endif endif
         else  0  endif }
	"Check for SpeclFtr:SFamSolarWH" 		ResProj:SpeclFeatrs:SFamSolarWH 		= { if ( SumAll( ResDHWSys:SFamSolarWH ) > 0.5 ) then 1 else 0 endif }
	"Write SpeclFtr:SFamSolarWH Msg" 		ResProj:SpeclFeatrs:SFamSolarWHMsg 	= { if ( SpeclFeatrs:SFamSolarWH > 0.5 ) then MaxAll( ResDHWSys:SFamSolarWH ) else 0 endif }

	; SDS 8/2/16 - added reporting of MFamSolarWH (tic #707)   - SAC 2/19/20 - revised logic for new DHWSolarSys assignment (tic #1198)
	"Check for ResDHWSys:MFamSolarWH" 			ResDHWSys:MFamSolarWH					=
	   {  if ( IfValidAnd( ResProj:IsMultiFamily > 0.5 ) .AND. IfValidAnd( FloorAreaServed[1] > 0.1 ) )
         then  if (     IfValidAnd( Proj:AnalysisVersion >= 190102 )    .AND. IfValidAnd( DHWSolarSysRef:ErrID = 0 )) then  2
               else if (IfValidAnd( Proj:AnalysisVersion >= 190102 )==0 .AND. IfValidAnd( SolFracAnnRpt > 0 ) ) then  1
               else  0  endif endif
         else  0  endif }
	"Check for SpeclFtr:MFamSolarWH" 		ResProj:SpeclFeatrs:MFamSolarWH 		= { if ( SumAll( ResDHWSys:MFamSolarWH ) > 0.5 ) then 1 else 0 endif }
	"Write SpeclFtr:MFamSolarWH Msg" 		ResProj:SpeclFeatrs:MFamSolarWHMsg 	= { if ( SpeclFeatrs:MFamSolarWH > 0.5 ) then MaxAll( ResDHWSys:MFamSolarWH ) else 0 endif }

	"Check for SpeclFtr:NEEAHtPumpWtrHtr"  ResProj:SpeclFeatrs:NEEAHtPumpWtrHtr     	= {	if ( SumAll( ResWtrHtr:IsNEEAHtPumpWtrHtr ) > 0.5 ) then 1 else 0 endif }	; SAC 7/5/17 - tic #805
	"Write SpeclFtr:NEEAHtPumpWtrHtrMsg"  	ResProj:SpeclFeatrs:NEEAHtPumpWtrHtrMsg  	= {	if ( SpeclFeatrs:NEEAHtPumpWtrHtr > 0.5 ) then 1 else 0 endif }	; SAC 7/5/17 - tic #805

	"Check for SpeclFtr:BatterySystem"  ResProj:SpeclFeatrs:BatterySystem     	= {	if (IfValidAnd( SimulateBattery > 0.5 ) .AND. IfValidAnd( Proj:BattGridHarmCredit > 0.5 )==0) then 1 else 0 endif }	; SAC 7/12/17 - tic #863
   ; SAC 2/20/18 - revised battery capacity decimal precision from (input)->2 (tic #921)
	"Write SpeclFtr:BatterySystemMsg"  	ResProj:SpeclFeatrs:BatterySystemMsg  	= {	if ( SpeclFeatrs:BatterySystem < 0.5 ) then UNDEFINED
	         else if (Proj:IsSFamResRuleset)
            then  Format( "Battery System: %s kWh", FltToStr( BattMaxCap, 2, 0, 0, 1 ) )
            else  Format( "Battery System: %s kWh", FltToStr( SumAll( Batt:MaxCap ), 2, 0, 0, 1 ) )  endif endif }	; SAC 7/12/17 - tic #863   ; update to reference Batt:MaxCap for CBECC - SAC 03/21/23

	"Check for SpeclFtr:SelfUtilCredit" ResProj:SpeclFeatrs:SelfUtilCredit   	= {	if (IfValidAnd( SimulateBattery > 0.5 ) .AND. IfValidAnd( Proj:BattGridHarmCredit > 0.5 )) then 1 else 0 endif }	   ; SAC 5/23/19 / SAC 5/24/19
	"Write SpeclFtr:SelfUtilCreditMsg"  ResProj:SpeclFeatrs:SelfUtilCreditMsg 	= {	if ( SpeclFeatrs:SelfUtilCredit < 0.5 ) then UNDEFINED
	                                                                              else  Format( "Battery System: %s kWh (Self Utilization Credit taken)", FltToStr( BattMaxCap, 2, 0, 0, 1 ) )  endif }
END
