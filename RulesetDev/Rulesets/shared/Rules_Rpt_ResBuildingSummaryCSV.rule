      else if ( MaxRevRef( ResHVACSys:Fan, ResHVACSys:CoolDuctedRpt  ) > 0.5 .AND.
                MaxRevRef( ResHVACSys:Fan, ResHVACSys:StatusArray    ) < 2.5 .AND.
                MaxRevRef( ResHVACSys:Fan, ResHVACSys:DuctStatusFlag ) < 3.5 .AND.
                MinRevRef( ResHVACSys:Fan, ResHVACSys:DuctStatusFlag ) > 1.5 )
      then  1     ; report for fans of Alt or Exist Sys w/ Alt or New Ducts
