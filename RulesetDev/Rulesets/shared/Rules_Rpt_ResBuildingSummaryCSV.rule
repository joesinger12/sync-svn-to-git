; SAC 10/22/14 - split out HERS/Special features rules out of 'Reporting_SummarizeModel' and into this new rulelist 'Reporting_HERS_SpecialFeaturesSummary'
;*************************************************************************
RULELIST  "Reporting_HERS_SpecialFeaturesSummary" 1 0 0 0
;*************************************************************************

;--------------------------------------------------------------------
; Create lists of Special Features and HERS Verification measures for reporting
;	mjb	2/20/14	populate HERSSummary array of messages
;--------------------------------------------------------------------
;
; some Project level HERS Verifications
;
	"Ensure ResProj:HERSProj exists"		ResProj:Action	= {	if (LocalCompAssigned( HERSProj )) then  UNCHANGED
												else  EvalRulelist( "CreateAndAssignAsUserDefined_Proj_HERSOther", 1 )  endif  }

	"Check for Exist. Conditions Verification" 	ResProj:HERSProj:VerifyExistCond = { if ( ExistingCondVerif > 0.5 ) then 1 else 0 endif }
	"Populate Ex. Conditions Verification msg"	ResProj:HERSProj:ExCondRptMsg = { if ( HERSProj:VerifyExistCond > 0.5 ) then 1 else 0 endif }

	"Check for QII" 				ResProj:HERSProj:QII = { if ( ResProj:InsulConsQuality > 1.5 ) then 1 else 0 endif }
	"Populate QII msg"				ResProj:HERSProj:QIIRptMsg = { if ( HERSProj:QII > 0.5 ) then 1 else 0 endif }

; SAC 9/4/13 - added logic to fix ACH50 defaulting bug (GC issue 169)
;	"Check for Low Bldg Leakage" 			ResProj:HERSProj:LowBldgLkg = { if ( ResProj:ACH50 < 5.0 ) then 1 else 0 endif }
;  mjb   9/15/14  - no bldg leakage verifications for multi-family
;	"Check for Low Bldg Leakage" 			ResProj:HERSProj:LowBldgLkg = { if ( ( ResProj:IsMultiFamily > 0.5 .AND. ResProj:ACH50 < 7.0 ) .OR.
;																								 ( ResProj:IsMultiFamily < 0.5 .AND. ResProj:ACH50 < 5.0 ) ) then 1 else 0 endif }

; mjb 1/30/15 - mod to lookup std. ACH50 from T24RClimateZoneCodeBase
;	"Check for Low Bldg Leakage" 			ResProj:HERSProj:LowBldgLkg = { if ( ResProj:IsMultiFamily < 0.5 .AND. ResProj:ACH50 < 5.0 ) then 1 else 0 endif }
; SAC 4/15/15 - adjust for MFam duplex/townhomes options (based on ResProj:IsMFamDupOrTownhm)
; SAC 5/3/16 - revised to check ACH50 vs. default that takes into account presence of VLLDCS (low-lkg ducts) (tic #627)
	"Check for Low Bldg Leakage" 			ResProj:HERSProj:LowBldgLkg = { if ( ( ResProj:IsMultiFamily < 0.5 .OR. ResProj:IsMFamDupOrTownhm > 0.5) .AND.
																								  ResProj:ACH50 < ResProj:DefaultACH50 ) then 1 else 0 endif }
																						;		  ResProj:ACH50 < T24RClimateZoneCodeBase:ACH50( "ClimateZone", Proj:CliZnNum, "CodeBase", Proj:StdDesignBase, "BldgType", Proj:BldgTypeTblStr ) ) then 1 else 0 endif }
	"Write Low Bldg Leakage Msg" 			ResProj:HERSProj:LowBldgLkgRptMsg = { if ( HERSProj:LowBldgLkg > 0.5 ) then FltToStr( ResProj:CFM50, 1, 0, 3 ) else UNCHANGED endif }

;	mjb	 mod to always set HERS IAQ flag to 1	9/12/13
;	mjb	mode to conditionally set HERS IAQ flag	6/17/14
	; SAC 12/29/14 - mod to ensure IAQFanRptMsg & IAQFan are set for case where RunScope = 'Addition and/or Alteration' (2) & AdditionCFA >= 1000
	; SAC 4/30/19 - modify logic to ensure that HERS IAQ fan message is triggered for all 2019+ models including an (SFam) ADU (tic #1071)
	"Populate IAQ Fan msg"							ResProj:HERSProj:IAQFanRptMsg  = {
	         if (EnergyCodeYearNum >= 2019 .AND. IfValidAnd( SFamADUIAQUI > 0 )) then  1
	         else if ( ResProj:RunScope == 1 ) then 
					if ( IsAddAlone == 0 ) then  1 
					else if ( AdditionCFA > 1000 ) then  1  else  0  endif endif
				else if (RunScope == 2 .AND. IfValidAnd( AdditionCFA > 1000 )) then  1
				else  0  endif endif endif   }
	"IAQ Fan - flag set same as msg" 		   ResProj:HERSProj:IAQFan = {  HERSProj:IAQFanRptMsg  	}	
	"Check for Centrally Integrated IAQ" 		ResProj:HERSProj:CentralIAQ = { if (IfValidAnd( IsMultiFamily < 0.5 ) .AND. ResProj:UnitIAQOption[1] == 3 ) then 1 else 0 endif }

	"Populate Central IAQ msg"			ResProj:HERSProj:CIAQRptMsg = { if (IfValidAnd( HERSProj:CentralIAQ > 0.5 )) then 1 else 0 endif }

   "Populate KitRangeHood flag"     ResProj:HERSProj:KitRangeHood =        ; SAC 10/29/19 - removed logic causing ALL New-NonAdditionAlone model from being flagged as having Kitchen (tic #1162)
      {  if (IfValidAnd( ProjInclKitchen > 0 )) then  1  else  0  endif  }     ; SAC 3/8/19 (tic #1085)
	"Populate KitRangeHood msg"      ResProj:HERSProj:KitRangeHoodMsg = { if (IfValidAnd( HERSProj:KitRangeHood > 0.5 )) then 1 else 0 endif }

	"Check for Fan Efficacy if CI-IAQ"		ResProj:HERSFan:AHUFanEff = { if (IfValidAnd( HERSProj:CentralIAQ > 0.5 )) then 1 else 0 endif }
;	"Check for Cool Vent System"			ResProj:HERSProj:CoolVent = { if (IfValidAnd( UnitClVentNumFans[1] > 0.5 )) then 1 else 0 endif }
;	"Check for Cool Vent System"			ResProj:HERSProj:CoolVent = { if (IfValidAnd( UnitClVentNumFans[2] > 0.5 )) then 1 else UNCHANGED endif }
;	"Check for Cool Vent System"			ResProj:HERSProj:CoolVent = { if (IfValidAnd( UnitClVentNumFans[3] > 0.5 )) then 1 else UNCHANGED endif }
;	"Check for Cool Vent System"			ResProj:HERSProj:CoolVent = { if (IfValidAnd( UnitClVentNumFans[4] > 0.5 )) then 1 else UNCHANGED endif }
;	"Check for Cool Vent System"			ResProj:HERSProj:CoolVent = { if (IfValidAnd( UnitClVentNumFans[5] > 0.5 )) then 1 else UNCHANGED endif }
;	"Check for Cool Vent System"			ResProj:HERSProj:CoolVent = { if (IfValidAnd( UnitClVentNumFans[6] > 0.5 )) then 1 else UNCHANGED endif }
;	"Populate Cool Vent msg"			ResProj:HERSProj:CoolVentRptMsg = { if (IfValidAnd( HERSProj:CoolVent > 0.5 )) then 1 else 0 endif }

   ; SAC 11/5/17 - added VerifyClVent & VerifyClVentRptMsg for 2019 analysis (tic #920)
   ; SAC 2/14/18 - revised to always report new HERS message for CFI ClVent (tic #977)
   ; SAC 4/30/19 - added logic to force HERS verif on for 'Specify Individual Whole House Fans' (tic #1023)
   ; JP  7/29/19 - added logic to prevent HERS verif on ClVentVerified = 1 AND UnitClVentOption = 0 condition (tic #1091)
	"Check for Verified ClVent"		ResProj:HERSProj:VerifyClVent       = {
         if (EnergyCodeYearNum < 2019 .OR. IsMultiFamily > 0.5) then  0
         else if (UnitClVentOption = 3 .OR. UnitClVentOption = 2) then  1
         else if (ClVentVerified = 0 .OR. UnitClVentOption = 0 .OR. IfValidAnd( UnitClVentCFM[1] <0.5))
         then  0  else  1  endif endif endif  }
   ; SAC 7/24/19 - added logic to expand 2019+ HERS messages to include Fixed vs. Variable CFI coolvent (tic #1104)
	"Populate Verified ClVent Msg"	ResProj:HERSProj:VerifyClVentRptMsg = {
         if (IfValidAnd( HERSProj:VerifyClVent > 0.5 ))
         then  if (UnitClVentOption = 3)
               then  if (SumAll( ResHVACSys:IsVariableCFIClVent ) > 0)
                     then  3
                     else  2  endif
               else  1  endif  
         else  0  endif }

	"Check for Zonal Cooling"			ResProj:HERSProj:ZonalCoolSys = { if ( Maxall( ResHVACSys:CoolSystem:IsZonal ) > 0.5 ) then 1 else 0 endif }
	"Populate Zonal Cooling"			ResProj:HERSProj:ZonalRptMsg = { if (IfValidAnd( HERSProj:ZonalCoolSys > 0.5 )) then 1 else 0 endif }
	; SAC 5/3/16 - added project-wide HERS ZonalSysBypassDuct (tic #634)
	"Check for Zonal systems w/ Bypass Ducts within Proj"		ResProj:HERSProj:ZonalSysBypassDuct = { if ( MaxAll( ResHVACSys:ZonalSysBypassDuct ) > 0.5 ) then 1 else 0 endif }
	"Write Proj Zonal systems w/ Bypass Ducts msg" 				ResProj:HERSProj:ZonalSysBypassDuctRptMsg = { if (MaxAll( ResHVACSys:ZonalSysBypassDuct ) > 0.5) then "Bypass duct conditions in zonal system(s)" else UNDEFINED endif }

	"Check for non-std Spray Foam Ins" 		ResConsAssm:RptNonStd = { if (IfValidAnd( AssignedCondZoneArea > 0.5 ) .AND. IfValidAnd( NonStdSprayFoam > 0.5 )) then 1 else 0 endif }
	"Check for High Spray Foam R-values"		ResProj:HERSProj:SprayFoamHighR = { if ( SumAll( ResConsAssm:RptNonStd ) > 0.5 ) then 1 else 0 endif }
	"Populate High Spray Foam msg"			ResProj:HERSProj:SprayFoamRptMsg = { if (IfValidAnd( HERSProj:SprayFoamHighR > 0.5 )) then 1 else 0 endif }

; SAC 3/9/17 - property to help ensure that HERS is triggered project-wide for certain combinations of HVACCool & HVACDist Status settings (tic #764)
   "Set ResHVACSys:IsAltSrvdDucted - whether this system is altered and ducted (and assigned to floor area)"
      ResHVACSys:IsAltSrvdDucted  = {
         if (IfValidAnd( FloorAreaServed > 0.5 )==0 .OR. Status != 2 .OR.
             IfValidAnd( HtOrClDucted > 0 )==0 .OR. LocalCompAssigned( DistribSystem )==0)
         then  0  else  1  endif  }
   "Set ResHVACSys:TriggerHERS"        ResHVACSys:TriggerHERS  = {
         if (IsAltSrvdDucted > 0 .AND.
             ( DistribSystem:Status == 1 .OR.
              (DistribSystem:Status == 4 .AND. IfValidAnd( DistribSystem:IsLessThanFortyFt > 0 ))))
         then  1  ; ensure HERS is triggered for Altered systems w/ Existing or Exist+New < 40 combos
         else  0  endif  }
   "Set ResHVACSys:DuctStatusFlag[1]"     ResHVACSys:DuctStatusFlag[1]  = {
         if (IfValidAnd( FloorAreaServed > 0.5 )==0 .OR. 
             IfValidAnd( HtOrClDucted > 0 )==0 .OR. LocalCompAssigned( DistribSystem )==0) then  0
         else if (DistribSystem:Status == 4)  ; "Existing + New"
         then  if (DistribSystem:IsLessThanFortyFt) then  4  else  5  endif
         else  DistribSystem:Status
         endif endif  }
   "Set ResHVACSys:DuctStatusFlag[2]"   ResHVACSys:DuctStatusFlag[2]  = {  DuctStatusFlag[1]  }
   "Set ResHVACSys:DuctStatusFlag[3]"   ResHVACSys:DuctStatusFlag[3]  = {  DuctStatusFlag[1]  }
   "Set ResHVACSys:DuctStatusFlag[4]"   ResHVACSys:DuctStatusFlag[4]  = {  DuctStatusFlag[1]  }
   "Set ResHVACSys:DuctStatusFlag[5]"   ResHVACSys:DuctStatusFlag[5]  = {  DuctStatusFlag[1]  }
   "Set ResHVACSys:DuctStatusFlag[6]"   ResHVACSys:DuctStatusFlag[6]  = {  DuctStatusFlag[1]  }
   "Set ResHVACSys:DuctStatusFlag[7]"   ResHVACSys:DuctStatusFlag[7]  = {  DuctStatusFlag[1]  }
   "Set ResHVACSys:DuctStatusFlag[8]"   ResHVACSys:DuctStatusFlag[8]  = {  DuctStatusFlag[1]  }
   "Set ResHVACSys:DuctStatusFlag[9]"   ResHVACSys:DuctStatusFlag[9]  = {  DuctStatusFlag[1]  }
   "Set ResHVACSys:DuctStatusFlag[10]"  ResHVACSys:DuctStatusFlag[10] = {  DuctStatusFlag[1]  }

;
; HVAC HERS Verification measures
;	mjb	mods needed to make work with HERSCool, HERSDist and HERSFan	9/1/13
;	mjb	add HERS flags to HVACHtPump objects	9/1/13
;  mjb	do not produce HVAC Cooling verification requirements when there is no cooling or no ducts	2/19/14
;	mjb	do not produce HVAC verification reqs. when equipment does not match what was modeled for Prop. Design	4/17/14
;
;"debug" Proj:Action = { StoreBEMProc( Format( "HERScr8s-b4-%s", RunAbbrev ), 0 ) }  ; SAC 05/05/23
	"Ensure HVACCool:HERSCheck exists"			HVACCool:Action	= {	if (LocalCompAssigned( HERSCheck )) then  UNCHANGED
																							else if (FloorAreaServed > 0.5) then 
																										EvalRulelist( "CreateAndAssignAsUserDefined_HVACCool_HERSCool", 1 )
																									else UNCHANGED endif
																							endif  }
   ; SAC 5/10/19 - added logic to create and populate HERSHtPump object for 2019 models
	"Ensure ResHtPumpSys:HERSCheck exists"		ResHtPumpSys:Action = {	if (LocalCompAssigned( HERSCheck ) .OR. LocalCompAssigned( HERSHtPumpRef )) then  UNCHANGED
																							else if (FloorAreaServed > 0.5)
																							then  if ( ResProj:EnergyCodeYearNum < 2019)
																							      then  EvalRulelist( "CreateAndAssignAsUserDefined_HVACHtPump_HERSCool", 1 )
																							      else  EvalRulelist( "CreateAndAssignAsUserDefined_HVACHtPump_HERSHtPump", 1 )  endif
																							else UNCHANGED endif
