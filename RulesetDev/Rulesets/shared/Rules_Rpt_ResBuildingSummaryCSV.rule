;*************************************************************************  SAC 5/10/19 - split out rules that populate HERSCool & HERSHeat for 2016 & earlier code vints vs. HERSHtPump for 2019+ models
RULELIST  "CreateAndAssignAsUserDefined_HVACHtPump_HERSCoolHeat_Populate" 1 0 0 0

;	mjb	7/17/14	modify HVACHtPump airflow check to set flag only for ducted systems, and set flag regardless of No Cooling system selection
;	"HtPump - Check for CFM per Ton" 		ResHtPumpSys:HERSCheck:AHUAirFlow = { if ( FloorAreaServed > 0.5 .AND. IfValidAnd(IsClSysCFMperTon > 0.5) ) then
;																											if ( Type > 1 .AND. Type < 16 ) then 1 else 0 endif 
;											     														else 0 endif }
;	"Check for CFM per Ton" 					ResHtPumpSys:HERSCheck:AHUAirFlow = { 	if ( FloorAreaServed > 0.5 .AND. IfValidAnd(IsClSysCFMperTon > 0.5) .AND. MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:CoolDuctedRpt ) > 0.5 .AND. MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:Status ) > 1.5 )
;																												then 1 else 0 endif	}
;	mjb	11/5/14	modify HtPump HERS rules so that ALL HtPump performance info is reported  issue #377
;	"Check for CFM per Ton" 					ResHtPumpSys:HERSCheck:AHUAirFlow = { 	if ( FloorAreaServed > 0.5 .AND. IfValidAnd(IsClSysCFMperTon > 0.5) .AND. MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:Status ) > 1.5 )
;																												then 1 else 0 endif	}
; SAC 4/21/15 - added check for IsClSysCFMperTon > 0.5 before setting AHUAirFlow
; SAC 4/23/15 - added check to ensure that CoolDuctedRpt is set for at least one system that references this equipment
; SAC 3/14/17 - applied same mod here as done for HVACCool on 8/28/16: added check to prevent setting HERS air flow for Altered systems where CFMperTon <= 350
; SAC 3/14/17 - added logic to ensure AHUAirFlow IS set for HtPump of Alt or Exist HVACSys combined w/ Alt or New Ducts (tic #764, runs 4,5,10,11)
; SAC 3/14/17 - added logic to ensure AHUAirFlow NOT set for HtPumps of New HVACSys combined w/ Existing Ducts (tic #764, run #6)
; SAC 6/12/18 - added logic to ensure AHUAirFlow NOT set for Cool of New HVACSys combined w/ Existing+New Ducts (tic #1022, DAR comment 6/7/18)
	"Check for CFM per Ton" 	ResHtPumpSys:HERSCheck:AHUAirFlow = {
	      if ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:HtPmpFlrAreaServed ) > 0.5 .AND.
			     MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:CoolDuctedRpt ) > 0.5 .AND.
              ( IfValidAnd( IsClSysCFMperTon > 0.5 ) .OR. 
                ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:DuctStatusFlag ) < 3.5 .AND.
                  MinRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:DuctStatusFlag ) > 1.5 ) ) )
         then  if ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:StatusArray ) < 2.5 .AND.
                    MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:DuctStatusFlag ) < 3.5 .AND.
                    MinRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:DuctStatusFlag ) > 1.5 )
               then  1     ; set reporting flag for Alt or Exist Sys w/ Alt or New Ducts
               else if (MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:StatusArray ) > 1.5)
               then  if (IsAltered > 0 .AND. IsNew < 1 .AND. IsExisting < 1 .AND. IfValidAnd( CFMperTon <= 350 ))
                     then  0
                     else if ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:StatusArray ) = 3 .AND.
                               ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:DuctStatusFlag ) = 1 .OR.
                                 MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:DuctStatusFlag ) > 3 ) )
                     then  0     ; not set for New HVACSys combined w/ Existing Ducts
                     else  1  endif endif
			      else  0  endif endif
			else  0  endif  }
	"HtPump - Write CFM per Ton Msg" 		ResHtPumpSys:HERSCheck:AirFlowRptMsg = { if (IfValidAnd( HERSCheck:AHUAirFlow > 0.5 )) then FltToStr( CFMperTon, 0, 0, 1 ) else UNDEFINED endif }

;	"HtPump - Check for SEER above Std" 	ResHtPumpSys:HERSCheck:SEER = { if ( FloorAreaServed  > 0.5 .AND. T24RCoolingEquipment:SEERValid( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) .AND. IfValidAnd(IsClSysSEER > 0.5) .AND. IfValidAnd(SEER > 0) ) then
;																									if (SEER > T24RCoolingEquipment:SEER( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr   ) ) then 1 else 0 endif
;											 													else  0  endif  }
; SAC 4/23/15 - added logic to NOT flag SEER in HERSCheck is this equipment is not modeled as the user entered it (look-up table PropEqStd = 1)
   ; revised for SEER2 - SAC 10/09/22 (tic #3431)
	"HtPump - Check for SEER above Std" 	ResHtPumpSys:HERSCheck:SEER = { if ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:HtPmpFlrAreaServed )  > 0.5 .AND.
																									  MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:StatusArray ) > 1.5 .AND.
																									  T24RCoolingEquipment:PropEqStd( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) < 0.5 ) then
																									if (EffMetric < 2 .AND. IfValidAnd(SEER > 0) .AND. 
                                                                               T24RCoolingEquipment:SEERValid( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ))
                                                                           then  if (SEER > ( T24RCoolingEquipment:SEER( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) + 0.005 ) ) then 1 else 0 endif
                                                                           else if (EffMetric > 1 .AND. IfValidAnd(SEER2 > 0) .AND. 
                                                                               T24RCoolingEquipment:SEER2Valid( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ))
                                                                           then  if (SEER2 > ( T24RCoolingEquipment:SEER2( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) + 0.005 ) ) then 1 else 0 endif
                                                                           else  0  endif endif
											 													else  0  endif  }
	"HtPump - Write SEER above Std Msg" 	ResHtPumpSys:HERSCheck:SEERRptMsg = { if (IfValidAnd( HERSCheck:SEER > 0.5 )) then FltToStr( SEER, 1, 0, 4 ) else UNDEFINED endif }

;	"HtPump - Check for EER above Std" 		ResHtPumpSys:HERSCheck:EER = {  if ( FloorAreaServed > 0.5 .AND. T24RCoolingEquipment:EERValid( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) .AND. LocalStatus(EER) < 7 .AND. IfValidAnd(IsClSysEER > 0.5) .AND. IfValidAnd(EER > 0) ) then
;																									if (EER > T24RCoolingEquipment:EER( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr   ) ) then 1 else 0 endif
;											 													else  0  endif  }
; SAC 4/23/15 - added logic to NOT flag EER in HERSCheck is this equipment is not modeled as the user entered it (look-up table PropEqStd = 1)
   ; revised for EER2 - SAC 10/09/22 (tic #3431)
	"HtPump - Check for EER above Std" 		ResHtPumpSys:HERSCheck:EER = {  if ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:HtPmpFlrAreaServed ) > 0.5 .AND. UseEERinAnalysis > 0.5 .AND. MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:StatusArray ) > 1.5 .AND.
																									  T24RCoolingEquipment:PropEqStd( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) < 0.5 )
                                                                        then  if ( EffMetric < 2 .AND. IfValidAnd( EER > 0.5) .AND. T24RCoolingEquipment:EERValid( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) )
																									   then  if (EER > ( T24RCoolingEquipment:EER( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) + 0.005 ) ) then 1 else 0 endif
                                                                              else if ( EffMetric > 1 .AND. IfValidAnd( EER2 > 0.5) .AND. T24RCoolingEquipment:EER2Valid( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) )
																									   then  if (EER2 > ( T24RCoolingEquipment:EER2( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) + 0.005 ) ) then 1 else 0 endif
                                                                              else  0  endif endif
											 													else  0  endif  }
	"HtPump - Write EER above Std Msg" 		ResHtPumpSys:HERSCheck:EERRptMsg = { if (IfValidAnd( HERSCheck:EER > 0.5 )) then FltToStr( EER, 1, 0, 4 ) else UNDEFINED endif }

; SAC 4/23/15 - added logic to NOT flag Refrig Charge in HERSCheck is this equipment is not modeled as the user entered it (look-up table PropEqStd = 1)
; SAC 5/4/15 - replaced check of T24RCoolingEquipment:PropEqStd() w/ simple check for GSHP system type, since that is the only system type that refrigerant charge reporting is disabled for (issue 502)
; SAC 6/18/18 - ROLL BACK this mod which broke some tic #764 runs - SAC 6/15/18 - restrict cases where this msg occurs to ONLY NEW HVACSys objects (HVACSys:StatusArray ) > 1.5 -->> = 3) (tic #981)
; added logic to prevent systems of specific (packaged & room) types from being flagged - SAC 09/04/20 (tic #1003)
	"HtPump - Check for AC Charge"			ResHtPumpSys:HERSCheck:ACCharg = {
         if ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:HtPmpFlrAreaServed ) > 0 .AND. ACCharge > 1.5 .AND.
              MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:StatusArray ) > 1.5 .AND.
              Type != 26 .AND. Type != 24 .AND. Type != 7 .AND. Type != 12 )     ; NOT PkgTermHeatPump, SglPkgVertHeatPump, RoomHeatPump or GroundSourceHeatPump
         then  1  else  0  endif }
	"HtPump - Write AC Charge Msg" 			ResHtPumpSys:HERSCheck:ACChargRptMsg = { if (IfValidAnd( HERSCheck:ACCharg > 0.5 )) then ACCharge else 0 endif }

   ; SAC 3/18/19 - added setting of ResHtPumpSys:HERSHeatRef properties for new 2019 HERS messaging
   ; revised for HSPF2 - SAC 10/09/22 (tic #3431)
   "Check for Pkg or Split HSPF above Std - ResHtPumpSys:HERSHeatRef:HSPF"   ResHtPumpSys:HERSHeatRef:HSPF = {
         if ( FloorAreaServed > 0.5 .AND. (IfValidAnd(IsNew > 0) .OR. IfValidAnd(IsAltered > 0)) .AND. (Type == 4 .OR. Type == 5) )  ; "SplitHeatPump - Central split heat pump" -or- "PkgHeatPump - Central packaged heat pump"
         then  if (EffMetric < 2 .AND. IfValidAnd( HSPF ) .AND. T24RHeatingEquipment:HSPFValid( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) > 0 )
               then  if (HSPF > T24RHeatingEquipment:HSPF( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) + 0.005)
                     then  1  else  0  endif
               else if (EffMetric > 1 .AND. IfValidAnd( HSPF2 ) .AND. T24RHeatingEquipment:HSPF2Valid( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) > 0 )
               then  if (HSPF2 > T24RHeatingEquipment:HSPF2( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) + 0.005)
                     then  1  else  0  endif
               else  0  endif endif
         else  0  endif  }
   "Write HSPF above Std Msg - ResHtPumpSys:HERSHeatRef:HSPFRptMsg"          ResHtPumpSys:HERSHeatRef:HSPFRptMsg = {
         if ( IfValidAnd( HERSHeatRef:HSPF > 0.5 ) ) then  "Verified HSPF"
         else  UNDEFINED  endif }
   ; original version sets flag only for "SplitHeatPump - Central split heat pump" -or- "PkgHeatPump - Central packaged heat pump"
   "Check for Pkg or Split heating capacity - ResHtPumpSys:HERSHeatRef:Capacity"   ResHtPumpSys:HERSHeatRef:Capacity = {
         if ( FloorAreaServed > 0.5 .AND. (IfValidAnd(IsNew > 0) .OR. IfValidAnd(IsAltered > 0)) .AND. 
              (Type == 4 .OR. Type == 5) .AND. IfValidAnd( AutoSize > 0 )==0 )
         then  1  else  0  endif  }
   "Write heating capacity Msg - ResHtPumpSys:HERSHeatRef:CapacityRptMsg"          ResHtPumpSys:HERSHeatRef:CapacityRptMsg = {
         if ( IfValidAnd( HERSHeatRef:Capacity > 0.5 ) ) then  "Verified heat pump rated heating capacity"
         else  UNDEFINED  endif }

END
;*************************************************************************  SAC 5/10/19 - split out rules that populate HERSCool & HERSHeat for 2016 & earlier code vints vs. HERSHtPump for 2019+ models
RULELIST  "CreateAndAssignAsUserDefined_HVACHtPump_HERSHtPump_Populate" 1 0 0 0

; DUPLICATED FROM HERSCool/Heat RULELIST ABOVE

	"Check for CFM per Ton" 	ResHtPumpSys:HERSHtPumpRef:AHUAirFlow = {
	      if ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:HtPmpFlrAreaServed ) > 0.5 .AND.
			     MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:CoolDuctedRpt ) > 0.5 .AND.
              ( IfValidAnd( IsClSysCFMperTon > 0.5 ) .OR. 
                ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:DuctStatusFlag ) < 3.5 .AND.
                  MinRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:DuctStatusFlag ) > 1.5 ) ) )
         then  if ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:StatusArray ) < 2.5 .AND.
                    MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:DuctStatusFlag ) < 3.5 .AND.
                    MinRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:DuctStatusFlag ) > 1.5 )
               then  1     ; set reporting flag for Alt or Exist Sys w/ Alt or New Ducts
               else if (MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:StatusArray ) > 1.5)
               then  if (IsAltered > 0 .AND. IsNew < 1 .AND. IsExisting < 1 .AND. IfValidAnd( CFMperTon <= 350 ))
                     then  0
                     else if ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:StatusArray ) = 3 .AND.
                               ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:DuctStatusFlag ) = 1 .OR.
                                 MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:DuctStatusFlag ) > 3 ) )
                     then  0     ; not set for New HVACSys combined w/ Existing Ducts
                     else  1  endif endif
			      else  0  endif endif
			else  0  endif  }
	"HtPump - Write CFM per Ton Msg" 		ResHtPumpSys:HERSHtPumpRef:AirFlowRptMsg = { if (IfValidAnd( HERSHtPumpRef:AHUAirFlow > 0.5 )) then FltToStr( CFMperTon, 0, 0, 1 ) else UNDEFINED endif }

	"HtPump - Check for SEER above Std" 	ResHtPumpSys:HERSHtPumpRef:SEER = { if ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:HtPmpFlrAreaServed )  > 0.5 .AND. T24RCoolingEquipment:SEERValid( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) .AND.
																									  IfValidAnd(SEER > 0) .AND. MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:StatusArray ) > 1.5 .AND.
																									  T24RCoolingEquipment:PropEqStd( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) < 0.5 ) then
																									if (SEER > ( T24RCoolingEquipment:SEER( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) + 0.005 ) ) then 1 else 0 endif
											 													else  0  endif  }
	"HtPump - Write SEER above Std Msg" 	ResHtPumpSys:HERSHtPumpRef:SEERRptMsg = { if (IfValidAnd( HERSHtPumpRef:SEER > 0.5 )) then FltToStr( SEER, 1, 0, 4 ) else UNDEFINED endif }

	"HtPump - Check for EER above Std" 		ResHtPumpSys:HERSHtPumpRef:EER = {  if ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:HtPmpFlrAreaServed ) > 0.5 .AND. T24RCoolingEquipment:EERValid( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr )  .AND. 
																									  UseEERinAnalysis > 0.5 .AND. IfValidAnd(EER > 0) .AND. MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:StatusArray ) > 1.5 .AND.
																									  T24RCoolingEquipment:PropEqStd( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) < 0.5 ) then
																									if (EER > ( T24RCoolingEquipment:EER( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) + 0.005 ) ) then 1 else 0 endif
											 													else  0  endif  }
	"HtPump - Write EER above Std Msg" 		ResHtPumpSys:HERSHtPumpRef:EERRptMsg = { if (IfValidAnd( HERSHtPumpRef:EER > 0.5 )) then FltToStr( EER, 1, 0, 4 ) else UNDEFINED endif }

   ; added logic to prevent systems of specific (packaged & room) types from being flagged - SAC 09/04/20 (tic #1003)
	"HtPump - Check for AC Charge"			ResHtPumpSys:HERSHtPumpRef:ACCharg = {
         if ( MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:HtPmpFlrAreaServed ) > 0 .AND. ACCharge > 1.5 .AND.
              MaxRevRef( ResHVACSys:HtPumpSystem, ResHVACSys:StatusArray ) > 1.5 .AND.
              Type != 26 .AND. Type != 24 .AND. Type != 7 .AND. Type != 12 )     ; NOT PkgTermHeatPump, SglPkgVertHeatPump, RoomHeatPump or GroundSourceHeatPump
         then  1  else  0  endif }
	"HtPump - Write AC Charge Msg" 			ResHtPumpSys:HERSHtPumpRef:ACChargRptMsg = { if (IfValidAnd( HERSHtPumpRef:ACCharg > 0.5 )) then ACCharge else 0 endif }

   ; SAC 5/10/19 - revised 2019 logic to trigger HtPump HSPF nad/or Capacity verif for any HP where PopEqStd = 0
   "Check for Pkg or Split HSPF above Std - ResHtPumpSys:HERSHtPumpRef:HSPF"   ResHtPumpSys:HERSHtPumpRef:HSPF = {
         if ( FloorAreaServed > 0.5 .AND. (IfValidAnd(IsNew > 0) .OR. IfValidAnd(IsAltered > 0)) .AND. 
              T24RHeatingEquipment:PropEqStd( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) == 0 .AND.
              T24RHeatingEquipment:HSPFValid( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) > 0 )
         then  if (ValidOr( HSPF, 0 ) > T24RHeatingEquipment:HSPF( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ))
               then  1
               else  0  endif
         else  0  endif  }
   "Write HSPF above Std Msg - ResHtPumpSys:HERSHtPumpRef:HSPFRptMsg"          ResHtPumpSys:HERSHtPumpRef:HSPFRptMsg = {
         if ( IfValidAnd( HERSHtPumpRef:HSPF > 0.5 ) ) then  "Verified HSPF"
         else  UNDEFINED  endif }
   ; switched logic for 2019.2.0+ to trigger flag for ALL HP types, not just those explicitly simulated - SAC 02/09/21 (tic #1254)
   "Check for Pkg or Split heating capacity - ResHtPumpSys:HERSHtPumpRef:Capacity"   ResHtPumpSys:HERSHtPumpRef:Capacity = {
         if ( FloorAreaServed > 0.5 .AND. (IfValidAnd(IsNew > 0) .OR. IfValidAnd(IsAltered > 0)) .AND. 
              ( (IfValidAnd( Proj:AnalysisVersion >= 190200 )==0 .AND. T24RHeatingEquipment:PropEqStd( "CodeBase", Proj:StdDesignBase, "StdVer", ResProj:StdsVersionYr, "SysType", TypeAbbrevStr ) == 0) .OR.
                (IfValidAnd( Proj:AnalysisVersion >= 190200 )    .AND. Type > 0) )  .AND.
              IfValidAnd( AutoSize > 0 )==0 )
         then  1  else  0  endif  }
   ; added logic to prevent systems of specific (packaged & room) types from being flagged for Capacity17 verification - SAC 09/04/20 (tic #1003)
   "Check for Pkg or Split heating capacity - ResHtPumpSys:HERSHtPumpRef:Capacity17"  ResHtPumpSys:HERSHtPumpRef:Capacity17 =
      {  if ( HERSHtPumpRef:Capacity > 0 .AND.
              Type != 26 .AND. Type != 24 .AND. Type != 7 .AND. Type != 12 )     ; NOT PkgTermHeatPump, SglPkgVertHeatPump, RoomHeatPump or GroundSourceHeatPump
         then  1  else  0  endif  }
   "Write heating capacity Msg - ResHtPumpSys:HERSHtPumpRef:CapacityRptMsg"          ResHtPumpSys:HERSHtPumpRef:CapacityRptMsg = {
         if ( IfValidAnd( HERSHtPumpRef:Capacity > 0.5 ) ) then  "Verified heat pump rated heating capacity"
         else  UNDEFINED  endif }
END
