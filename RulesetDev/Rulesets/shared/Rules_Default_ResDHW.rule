         T24RClimateZoneCodeBase:ElecDHWDrnWtrHtRecovShwrSrvd( "ClimateZone", Proj:CliZnNum, "CodeBase", Proj:StdDesignDrnWtrHtRecYear, "BldgType", Proj:BldgTypeTblStr ) / 100  }
